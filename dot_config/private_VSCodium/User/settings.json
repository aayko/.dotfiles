{
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["u"],
            "after": [],
            "commands": [
                {
                    "command": "undo",
                    "args": []
                }
            ]
        },
        {
            "before": ["<S-u>"],
            "after": [],
            "commands": [
                {
                    "command": "redo",
                    "args": []
                }
            ]
        },
    ],
    "vim.normalModeKeyBindings": [
        {
            "before": ["s"],
            "after": ["V"]
        }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": ["s"],
            "after": ["V"]
        }
    ],
    "vim.cursorStylePerMode.visualline": "block",
    "vim.highlightedyank.enable": true,
    "vim.replaceWithRegister": true,
    "vim.leader": "space",
    "vim.useSystemClipboard": true,
    "editor.lineNumbers": "relative",
    "editor.selectionClipboard": false,
    "notebook.compactView": false,
    "notebook.consolidatedRunButton": true,
    "notebook.dragAndDropEnabled": false,
    "security.workspace.trust.banner": "never",
    "notebook.undoRedoPerCell": false,
    "editor.fontFamily": "'Inconsolata Nerd Font', 'monospace', monospace",
    "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?_",
    "editor.minimap.enabled": false,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "hidden"
    },
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.vertical": "hidden",
    "window.zoomLevel": 2,
    "workbench.startupEditor": "none",
    "editor.matchBrackets": "never",
    "editor.occurrencesHighlight": "off",
    "editor.renderLineHighlight": "none",
    "workbench.editor.showTabs": "none",
    "editor.fontSize": 18,
    "editor.padding.top": 10,
    "editor.folding": false,
    "editor.renderWhitespace": "none",
    "window.menuBarVisibility": "toggle",
    "security.workspace.trust.startupPrompt": "never",
    "security.workspace.trust.enabled": false,
    "workbench.colorTheme": "Gruvbox Dark Hard",
    "workbench.activityBar.location": "hidden",
    "terminal.external.linuxExec": "st",
    "explorer.confirmDelete": false,
    "terminal.integrated.sendKeybindingsToShell": true,
    "workbench.colorCustomizations": {
        "[Gruvbox Dark Hard]": {
            "editor.background": "#0f0f0f",
            "activityBar.background": "#0f0f0f",
            "statusBar.background": "#0f0f0f",
            "sideBar.background": "#0f0f0f",
            "commandCenter.background": "#0f0f0f",
            "editorSuggestWidget.background": "#0f0f0f",
            "terminal.background": "#0f0f0f",
            "quickInput.background": "#0f0f0f"
        }
    },
    "breadcrumbs.enabled": false,
    "editor.scrollbar.verticalScrollbarSize": 0,
    "workbench.statusBar.visible": false,
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingQuotes": "never"
}